<configuration>
  <appender name="STDOUT"
    class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>
        %msg%n
     </pattern>
    </encoder>
  </appender>

  <logger name="org.preferanto.experiment.trial.perf" level="INFO" />

  <root level="WARN">
    <appender-ref ref="STDOUT" />
  </root>
</configuration>
